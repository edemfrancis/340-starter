<% if (title) { %>
  <h1>
    <%= title %>
  </h1>
  <% } else { res.redirect('/') } %>

    <!-- displays messages -->
    <%- messages() %>

          <% let account=locals.accountData %>
            <% if (locals.loggedin) { %>
              <h2 class="account_h2">Welcome <%= locals.accountData.account_firstname %></h2>
              <div class="management-links">
                <a href="/account/edit/<%= locals.accountData.account_id %>">Manage Your Account Information</a>
              </div>
              <h2 class="account_h2">Account Messaging</h2>
              <p class="account_p">you have <%= locals.unreadMessages %> unread messages</p>
              <div class="management-links">
                <a href="/inbox/">Inbox</a>
              </div>
              <div class="management-links">
              </div <% if (account.account_type=="Employee" || account.account_type=="Admin" ) { %> >
              <h2 class="account_h2">Inventory Management</h2>
              <div class="management-links">
                <a href="/inv/">Manage inventory</a>
              </div>
              <% } %>
                <% } %>